{
  "manifest_version": 3,
  "name": "Twitch Quality Adjuster",
  "version": "1.0",
  "description": "Automatically lower the quality of Twitch clips.",
  "permissions": ["scripting", "webNavigation"],
  "host_permissions": ["https://www.twitch.tv/*"],
  "background": {
    "service_worker": "background.js"
  }
}
